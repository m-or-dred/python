def busca(x, s):
    
    if s == []:
        return False    
    
    m = s[len(s)//2]
    
    if x == m:
        return True; print('está na posição', s.index(m))
    
    elif x < m:
        return busca(x, [_ for _ in s if _ < m])
    
    else:
        return busca(x, [_ for _ in s if _ > m])
