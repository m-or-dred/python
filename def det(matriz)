from random import randint;  n = randint(1,5)

M = [[randint(1,9) for j in range(n)] for i in range(n)]

print(M)
# for i in M:  print(i)
# print()

def Laplace(matriz):

	if len(matriz) > 1:
	
		det = 0
		
		for k in range(len(matriz)):
			
			R = [[matriz[i][j] for j in range(len(matriz)) if j != k] for i in range(1, len(matriz))]
			
			det += (-1) ** k * matriz[0][k] * Laplace(R)

		return det

	else:  return matriz[0][0]

print(Laplace(M))
